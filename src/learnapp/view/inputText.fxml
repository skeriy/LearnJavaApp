<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.TextFlow?>

<AnchorPane fx:id="practiceAnchorPane" prefHeight="600.0" prefWidth="800.0" stylesheets="@../style/style.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="learnapp.controller.InputTextController">
   <children>
      <Button fx:id="backToMenuBtn" layoutX="99.0" layoutY="499.0" mnemonicParsing="false" onAction="#onBackToMenu" styleClass="back_btn" text="назад к темам" />
      <Button fx:id="checkBtn" layoutX="596.0" layoutY="499.0" mnemonicParsing="false" onAction="#onCheck" styleClass="check_btn" text="Проверить" />
      <VBox fx:id="codeVBox" layoutX="154.0" layoutY="196.0" prefHeight="250.0" prefWidth="493.0" styleClass="practice_result" />
      <FlowPane layoutX="154.0" layoutY="98.0" prefHeight="56.0" prefWidth="493.0" styleClass="practice_text_box">
         <children>
            <TextFlow fx:id="inputTextExText" prefWidth="490.0">
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding></TextFlow>
         </children>
      </FlowPane>
      <HBox fx:id="navigateHBox" alignment="TOP_CENTER" prefHeight="40.0" prefWidth="800.0" />
   </children>
</AnchorPane>
