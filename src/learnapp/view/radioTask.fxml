<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="practiceAnchorPane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="learnapp.controller.RadioTaskController">
   <children>
      <Button fx:id="backToMenuBtn" layoutY="1.0" mnemonicParsing="false" onAction="#onBackToMenu" text="backToMenu" />
      <VBox fx:id="radioBtnBox" layoutX="104.0" layoutY="403.0" prefHeight="73.0" prefWidth="491.0">
         <children>
            <RadioButton fx:id="rad1" mnemonicParsing="false" text="r1" />
            <RadioButton fx:id="rad2" mnemonicParsing="false" text="r2" />
            <RadioButton fx:id="rad3" mnemonicParsing="false" text="r3" />
            <RadioButton fx:id="rad4" mnemonicParsing="false" text="r4" />
         </children></VBox>
      <Button fx:id="checkBtn" layoutX="709.0" layoutY="544.0" mnemonicParsing="false" onAction="#onCheck" text="Check" />
      <FlowPane layoutX="104.0" layoutY="46.0" prefHeight="330.0" prefWidth="622.0">
         <children>
            <Text fx:id="radioExText" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
         </children>
      </FlowPane>
   </children>
</AnchorPane>
